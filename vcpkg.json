{
    "$schema": "https://raw.githubusercontent.com/microsoft/vcpkg/master/scripts/vcpkg.schema.json",
    "name": "citra-emu",
    "version-string": "0.0.0",
    "dependencies": [
        "boost-serialization",
        "boost-system",
        "boost-range",
        "boost-icl",
        "boost-date-time",
        "boost-crc",
        "boost-container",
        "boost-asio",
        "catch2",
        "cpp-jwt",
        "cryptopp",
        "discord-rpc",
        "enet",
        "fmt",
        {
            "name": "glad",
            "features": [
                "gl-api-33",
                "gles2-api-32",
                "extensions"
            ]
        },
        "glm",
        {
            "name": "ffmpeg",
            "features": [
                "avcodec",
                "avformat",
                "avdevice",
                "swresample"
            ]
        },
        "fdk-aac",
        "cpp-httplib",
        "libressl",
        "inih",
        "nlohmann-json",
        "libusb",
        "libyuv",
        "lodepng",
        "glslang",
        "xbyak",
        "zstd",
        "sdl2"
    ]
}